acl = {
  enabled = true
  default_policy = "deny"
  down_policy = "deny"
  enable_token_persistence = true
  enable_token_replication = true

{% if server_token_json is defined %}

  tokens {
    agent = "{{(server_token_json.stdout | from_json)['SecretID']}}"
    replication = "{{replication_secret_id}}"
  }
{% endif %}

}